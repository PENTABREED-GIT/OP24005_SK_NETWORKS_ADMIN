<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.skn.admin.business.mapper.BusinessAreaMapper">
    <select id="selectBusinessAreaList"
            resultType="com.skn.admin.business.dto.BusinessArea">
        SELECT * FROM BUSINESS_AREA
        WHERE IS_OPEN='Y'
        ORDER BY BUSINESS_AREA_INDEX ASC
    </select>
</mapper>